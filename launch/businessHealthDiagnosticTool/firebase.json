{
  "functions": {
    "source": "functions"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/analyzeBusiness",
        "function": "analyzeBusiness"
      }
    ]
  },
  "firestore": {
    "rules": "firestore.rules"
  }
}
